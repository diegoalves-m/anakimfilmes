<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/gerenciamento.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cadFunc</from-outcome>
            <to-view-id>/funcionarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gerenFunc</from-outcome>
            <to-view-id>/gerenciamentoFuncionarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cadClien</from-outcome>
            <to-view-id>/clientes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gerenClien</from-outcome>
            <to-view-id>/gerenciamentoClientes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cadFilm</from-outcome>
            <to-view-id>/filmes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gerenFilm</from-outcome>
            <to-view-id>/gerenciamentoFilmes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>locacao</from-outcome>
            <to-view-id>/locacao.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>devolucao</from-outcome>
            <to-view-id>/devolucao.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/funcionarios.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backGeren</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/gerenciamentoFuncionarios.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backGeren</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>semlogin</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/clientes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backGeren</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/gerenciamentoClientes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backGeren</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/filmes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backGeren</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/devolucao.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backGeren</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/gerenciamentoFilmes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backGeren</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/locacao.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backGeren</from-outcome>
            <to-view-id>/gerenciamento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/locacoesCliente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>backDevCliente</from-outcome>
            <to-view-id>/devolucao.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
